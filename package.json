{
  "name": "filterdb",
  "version": "0.1.0",
  "description": "filterdb is an fully extendable no-SQL database completly written in node.js. Data could be accessed with JS-Array like find/filter Syntax. So there is no need to learn a new QueryLanguage.",
  "type": "module",
  "main": "lib/Database.js",
  "scripts": {
    "test": "c8 --check-coverage --lines 50 --functions 50 --branches 50 mocha ./test/**/*.js",
    "ci": "c8 --check-coverage --lines 50 --functions 50 --branches 50 --reporter=lcovonly mocha ./test/**/*.js && codecov",
    "release": "np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbittmann/filterdb.git"
  },
  "author": "sbittmann",
  "keywords": ["stream", "database", "db", "store", "storage", "json"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sbittmann/filterdb/issues"
  },
  "homepage": "https://github.com/sbittmann/filterdb#readme",
  "dependencies": {
    "acorn": "^8.5.0",
    "axios": "^0.23.0",
    "fastify": "^3.22.0",
    "lmdb-store": "^1.6.9",
    "minimist": "^1.2.5",
    "nanoid": "^3.1.30",
    "vm2": "^3.9.5",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "c8": "^7.10.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "faker": "^5.5.3",
    "mocha": "^9.1.3",
    "np": "^7.5.0",
    "prettier": "^2.4.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
